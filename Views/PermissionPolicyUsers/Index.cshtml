@model IEnumerable<TylorShop.Models.PermissionPolicyUser>

@{
    ViewData["Title"] = "Index";
}

<h1>Index</h1>

<p>
    <a asp-action="Create">Create New</a>
</p>
<table class="table">
    <thead>
        <tr>
            <th>
                @Html.DisplayNameFor(model => model.StoredPassword)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.ChangePasswordOnFirstLogon)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.UserName)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.IsActive)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.OptimisticLockField)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Gcrecord)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.ObjectTypeNavigation)
            </th>
            <th></th>
        </tr>
    </thead>
    <tbody>
@foreach (var item in Model) {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.StoredPassword)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.ChangePasswordOnFirstLogon)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.UserName)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.IsActive)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.OptimisticLockField)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Gcrecord)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.ObjectTypeNavigation.Oid)
            </td>
            <td>
                <a asp-action="Edit" asp-route-id="@item.Oid">Edit</a> |
                <a asp-action="Details" asp-route-id="@item.Oid">Details</a> |
                <a asp-action="Delete" asp-route-id="@item.Oid">Delete</a>
            </td>
        </tr>
}
    </tbody>
</table>
